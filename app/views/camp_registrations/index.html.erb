<h1>Listing camp_registrations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Position</th>
    <th>Email</th>
    <th>Phone</th>
    <th>School</th>
    <th>Comments</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @camp_registrations.each do |camp_registration| %>
  <tr>
    <td><%= camp_registration.name %></td>
    <td><%= camp_registration.age %></td>
    <td><%= camp_registration.position %></td>
    <td><%= camp_registration.email %></td>
    <td><%= camp_registration.phone %></td>
    <td><%= camp_registration.school %></td>
    <td><%= camp_registration.comments %></td>
    <td><%= link_to 'Show', camp_registration %></td>
    <td><%= link_to 'Edit', edit_camp_registration_path(camp_registration) %></td>
    <td><%= link_to 'Destroy', camp_registration, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

