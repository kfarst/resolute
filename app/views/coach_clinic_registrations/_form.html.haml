= form_for(@coach_clinic_registration) do |f|
  - if @coach_clinic_registration.errors.any?
    #error_explanation
      %h2
        = pluralize(@coach_clinic_registration.errors.count, "error")
        prohibited this registration from being submitted:
      %ul
        - @coach_clinic_registration.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .field
    = f.label "School/Program"
    %br/
    = f.text_field :program
  .field
    = f.label :email
    %br/
    = f.text_field :email
  .field
    = f.label :comments
    %br/
    = f.text_area :comments
  .actions
    = f.submit "Register"
