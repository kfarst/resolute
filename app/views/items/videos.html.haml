#title<
  Videos

- videos = $youtube.videos_by(:most_viewed)

- videos.videos.each do |video|
  #video
    %img{:id => "thumbnail", :src => video.thumbnails.first.url, :video_url => video.embed_url }
    #video_info
      %span{:id => "video_title"}= video.title 
      %span{:id => "video_description"}= video.description 

%div.clear
